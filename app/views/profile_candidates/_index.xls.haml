- if params[:filter].eql?('all')
  %table
    %thead
      %tr
        %th Id
        %th Nama
        %th Dinilai oleh
        %th Nilai CV
        %th Nilai Essai
        %th Nilai Surat Rekomendasi
        %th Total
        %th Keterangan
        %th Tipologi
        %th Asal Institusi
        %th Provinsi
        %th Surat rekomendasi
        %th Tanggal lahir
        %th Tempat lahir
        %th Alamat
        %th Gender
        %th Agama
        %th Handphone
        %th Hobi
        %th Gol darah
        %th Angkatan
        %th Info dari
        %th Daftar ke
        %th Pertunjukan
        %th Facebook
        %th Twitter
        %th Email
        %th FC Ekonomi Pembangunan
        %th FC Kebijakan Publik
        %th FC Energi dan Lingkungan
        %th FC Pendidikan dan Parenting
        %th FC Media Literasi
        %th FC Sosial Enterpreneurship
        %th FC Pangan Gizi dan Kesehatan
        %th FC Traventure
        %th FC People Development
        %th FC Liberal Arts
        %th Pilihan Jalur Peserta
        %th Komitmen ke FIM Pusat
        %th Komitmen ke FIM Regional
        %th Komitmen ke Bantu FIM dengan Komunitas Sendiri
    %tbody
      - profiles.each_with_index do |profile, index|
        %tr
          %td= profile.id
          %td= link_to(profile.fullname, profile.photo)
          %td= !profile.marked_by.nil? ? profile.marked_by.email : ""
          
          %td= ProfileCandidate.to_alphabet(profile.cv_point)
          %td= ProfileCandidate.to_alphabet(profile.essay_point)
          %td= ProfileCandidate.to_alphabet(profile.recommendation_letter_point)  
          %td= profile.total_point
          %td= profile.special_location_comment
          %td= profile.special_character_comment
          %td= profile.school
          %td= profile.province
          %td= link_to('Download', profile.recommendation_letter)
          %td= profile.dob.strftime("%d %B, %Y")
          %td= profile.place_of_birth
          %td= profile.location
          %td= profile.gender == "0" ? 'Laki-laki' : 'Perempuan'
          %td= profile.religion
          %td= "'#{profile.phone}"
          %td= profile.hobby
          %td= profile.blood_type
          %td= profile.batch
          %td= profile.information_from
          %td= profile.application_count
          %td= profile.performance_type
          %td= profile.facebook_url
          %td= profile.twitter? ? link_to(profile.twitter, "https://twitter.com/#{profile.twitter}") : "-"
          %td= profile.user.email
          %td= profile.diskusi_ekonomipembangunan
          %td= profile.diskusi_kebijakanpublik
          %td= profile.diskusi_energidanlingkungan
          %td= profile.diskusi_pendidikandanparenting
          %td= profile.diskusi_medialiterasi
          %td= profile.diskusi_socialentre
          %td= profile.diskusi_pangangizikesehatan
          %td= profile.diskusi_travel
          %td= profile.diskusi_peopledev
          %td= profile.diskusi_liberalarts
          - if profile.choose_type == 0 then
            %td Strategic Leader
          - elsif profile.choose_type == 1 then
            %td Local Leader
          - elsif profile.choose_type == 2 then
            %td Activist Based On Character
          %td= profile.is_committed_to_central_fim == true ? 'Iya' : 'Tidak'
          %td= profile.is_committed_to_regional_fim == true ? 'Iya' : 'Tidak'
          %td= profile.is_committed_to_own_organization == true ? 'Iya' : 'Tidak'
